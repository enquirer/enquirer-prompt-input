{
  "name": "enquirer-prompt-input",
  "description": "Input prompt plugin for Enquirer. This is the only prompt type included in Enquirer by default and does not need to be registered separately.",
  "version": "0.2.0",
  "homepage": "https://github.com/enquirer/enquirer-prompt-input",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "enquirer/enquirer-prompt-input",
  "bugs": {
    "url": "https://github.com/enquirer/enquirer-prompt-input/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "LICENSE",
    "README.md"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "log-utils": "^0.2.1",
    "prompt-base": "^0.3.2"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.10"
  },
  "keywords": [
    "answer",
    "answers",
    "ask",
    "bar",
    "bottom",
    "checkbox",
    "choice",
    "cli",
    "command",
    "enquirer",
    "input",
    "inquire",
    "inquirer",
    "interact",
    "list",
    "menu",
    "password",
    "prompt",
    "prompts",
    "question",
    "readline",
    "stdin",
    "stdout",
    "terminal",
    "tty",
    "ui"
  ],
  "verb": {
    "toc": false,
    "layout": "common-minimal",
    "data": {
      "alias": "input"
    },
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "enquirer",
        "enquirer-prompt",
        "enquirer-question"
      ]
    },
    "reflinks": [
      "enquirer",
      "verb",
      "verb-generate-readme"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
